{
  "private": true,
  "workspaces": [
    "web",
    "leftonread"
  ],
  "scripts": {
    "lint": "yarn workspaces run lint"
  },
  "dependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx|json|css)": [
      "echo 1"
    ]
  }
}
